  ## Default
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: -1f@1
    b: 0

  ## Value
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: 0f@1
    b: 1

  ## Value
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: 1f@1
    b: 1

  ## Value
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: 2f@1
    b: 8

  ## Proxied: no time mapping
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: 3f@1
    b: 3

  ## Proxied: no time offset
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_node
      time: 7f@2
    b: 3.5

  ## Time offset
  - !Equal/Real
    a: !AtTime/Real
      source: *frame_list_reset_time
