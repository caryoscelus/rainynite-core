## Value to test
_no_such_value: &frame_list_node !FrameList/Real
  frame_list: &frame_list !List/Frame/Real
    - !Frame/Real
      time: 0f@1
      value: 1
    - !Frame/Real
      time: 2f@1
      value: 8
    - !Frame/Real
      time: 3f@1
      value: !Linear
        speed: 1

_another_one: &frame_list_reset_time !FrameList/Real
